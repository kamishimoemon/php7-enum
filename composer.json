{
	"name": "kamishimoemon/php7-enum",
	"description": "Java-style enum system for PHP 7.4, with object-oriented design and multiple implementation strategies.",
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Dar√≠o Candotti",
			"email": "dario.candotti@gmail.com"
		}
	],
	"require": {
		"php": "^7.4"
	},
	"autoload": {
		"psr-4": {
			"PHP\\": "src/",
			"PHP\\Examples\\": "examples/"
		},
		"files": [
			"src/enumval.php"
		]
	},
	"require-dev": {
		"phpunit/phpunit": "^9.5"
	},
	"autoload-dev": {
		"psr-4": {
			"PHP\\Test\\": "test/"
		}
	},
	"scripts": {
		"test": "phpunit test --colors=always --testdox",
		"planets": "php -r 'require \"vendor/autoload.php\"; PHP\\Examples\\Planet::main();'",
		"daysoftheweek": "php -r 'require \"vendor/autoload.php\"; PHP\\Examples\\DayOfTheWeek::main();'"
	},
	"minimum-stability": "stable"
}
